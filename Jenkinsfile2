pipeline {
    agent any

    stages {
        stage('Run SSH Command on Remote Server') {
            steps {
                 sshagent (credentials: ['pem_key1']) {
                   sh 'ssh -o StrictHostKeyChecking=no -l ubuntu 43.205.203.235 ls -lrt uname -a'
                   }
                
                
                
                
            }
        }

        
    }
}
