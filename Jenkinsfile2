pipeline {
    agent any

    stages {
        stage('Run SSH Command on Remote Server') {
            steps {
            
                 sshagent (credentials: ['SSH_PEM'])  {
                  sh 'ssh -o StrictHostKeyChecking=no -l ubunut 65.2.184.189 ls'
                }
                
                
                
                
            }
        }

        
    }
}
