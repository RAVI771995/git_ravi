pipeline {
    agent any

    stages {
        stage('Run SSH Command on Remote Server') {
            steps {
                 sshagent(credentials: ['SSH_PEM']) {
                   sh 'ssh -o StrictHostKeyChecking=no -l ubuntu 65.0.108.244 ls'
                   }
                
                
                
                
            }
        }

        
    }
}
